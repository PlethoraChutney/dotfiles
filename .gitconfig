[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	last = log -1 HEAD
	unstage = reset HEAD --
	loggo = log --oneline --all --graph
[user]
	email = rich@posert.com
	name = Rich
[core]
	editor = vim
[pull]
	ff = only
[advice]
	addIgnoredFile = false
[push]
	followTags = true
[submodule]
	recurse = true
